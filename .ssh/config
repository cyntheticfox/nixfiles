# This file will need to be renamed to "config" before use in .ssh

# Default configuration
Host *
  ForwardAgent no
  ForwardX11 no
  PasswordAuthentication yes
  PubkeyAuthentication yes
  HashKnownHosts yes
  IdentityFile ~/.ssh/id_ed25519

Host github github.com
  HostName github.com
  User git
  Compression yes
  IdentityFile ~/.ssh/github_id_ed25519
  Port 22
