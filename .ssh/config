Host *
  ForwardAgent no
  ForwardX11 no
  PasswordAuthentication yes
  PubkeyAuthentication yes
  HashKnownHosts yes
  IdentityFile ~/.ssh/id_ed25519

Host github github.com
  HostName github.com
  User git
  Compression yes
  Port 22
  IdentityFile ~/.ssh/github_id_ed25519

Host gitlab gitlab.com
  HostName gitlab.com
  User git
  Compression yes
  Port 22
  IdentityFile ~/.ssh/gitlab_id_ed25519
